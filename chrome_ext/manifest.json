    "manifest_version": 3,
    "name": "Insight Bot",
    "version": "1.0",
    "description": "Get youtube video insights.",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["http://localhost:5000/*"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}
